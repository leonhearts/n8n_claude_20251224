      "name": "Generate Video",
      "type": "n8n-nodes-base.executeCommand",
      "typeVersion": 1,
      "position": [896, 112]
    },
    {
      "parameters": {
        "jsCode": "// スクリプトの出力をパース\nconst output = $input.first().json.stdout;\n\ntry {\n  const result = JSON.parse(output);\n  \n  if (result.success && result.localVideoPath) {\n    return [{\n      json: {\n        success: true,\n        videoUrl: result.videoUrl,\n        localPath: result.localVideoPath,\n        projectUrl: result.projectUrl,\n        generationTime: result.generationTime,\n        fileName: result.localVideoPath.split('/').pop()\n      }\n    }];\n  } else {\n    throw new Error(result.error || 'Video generation failed');\n  }\n} catch (e) {\n  throw new Error('Parse error: ' + e.message + ' | Raw: ' + output.substring(0, 200));\n}"
      },
      "id": "3de10a5b-8b9b-49b2-adc5-6c9b5c12a367",
      "name": "Parse Result",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1088, 112]
    },
    {
      "parameters": {
        "operation": "read",
        "fileSelector": "={{ $json.localPath }}",
